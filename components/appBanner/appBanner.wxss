.app-list {
    position: relative;
    background-color: hsla(0,0%,100%,.8);
}

.app-list .content {
    min-width: 100%;
    height: 170rpx;
}

.app-list-item {
    width: 177rpx;
    overflow: hidden;
    z-index: 2;
}

.app-list .thumbnail {
    width: 80rpx;
    height: 80rpx;
    border-radius: 100%;
}

.app-list .name {
    height: 40rpx;
    overflow: hidden;
}