.page {
    background-size: 100% 100%;
    overflow: hidden;
    border: solid 1px red;
    display: flex;
}

.page .head-item>image {
    z-index: 1;
}

.page .head-item-value,.page .head-item-value2 {
    background-color: #b3978c;
    color: #fff;
}

.page .head-item-value {
    margin-left: -6rpx;
    padding: 6rpx 6rpx 6rpx 16rpx;
    border-top-right-radius: 40rpx;
    border-bottom-right-radius: 40rpx;
}

.page .head-item-value2 {
    margin-left: -30rpx;
    padding: 6rpx 6rpx 6rpx 40rpx;
    border-radius: 40rpx;
}

.page .head-item .animation {
    animation: zoom2 .8s linear infinite;
}

@-webkit-keyframes zoom2 {
    0% {
        transform: scale(1);
    }

    50% {
        transform: scale(1.25);
    }

    100% {
        transform: scale(1);
    }
}

@keyframes zoom2 {
    0% {
        transform: scale(1);
    }

    50% {
        transform: scale(1.25);
    }

    100% {
        transform: scale(1);
    }
}

.page .float-task {
    width: 60uxp;
    background-color: #fff;
    box-shadow: 0 0 10rpx 10rpx rgba(0,0,0,.1);
    font-size: 28rpx;
    border-top-right-radius: 20rpx;
    border-bottom-right-radius: 20rpx;
    position: fixed;
    left: 0;
    top: 40%;
    z-index: 9;
}

.page .float-task .dot {
    width: 20rpx;
    height: 20rpx;
    background-color: #f72526;
    border-radius: 100%;
    margin-bottom: 6rpx;
    animation: flash .6s linear infinite;
}

.page .home .msg {
    min-width: 500rpx;
    height: 46rpx;
    border: 2rpx solid #f72526;
    color: #f72526;
    border-radius: 40rpx;
}

.page .home .start {
    animation: zoom 1s ease-in-out infinite;
}

.page .home .role {
    width: 537rpx;
    height: 392rpx;
    overflow: hidden;
    position: relative;
}

.page .home .role-img {
    width: 1074rpx;
    height: 392rpx;
    position: absolute;
    top: 0;
    left: 0;
}

.page .home .role-img2 {
    width: 186rpx;
    height: 392rpx;
    position: absolute;
    top: 0;
    left: 176rpx;
}

.page .home .role .s1 {
    left: 0;
}

.page .home .role .s2 {
    left: -537rpx;
}

.page .home .start {
    width: 500rpx;
    height: 116rpx;
    position: relative;
}

.page .home .start-image {
    width: 100%;
    height: 100%;
}

.page .home .float-icon {
    width: 104rpx;
    height: 123rpx;
}

.page .home .ranking {
    left: 20rpx;
    bottom: 24%;
}

.page .home .invite {
    right: 20rpx;
    bottom: 24%;
}

.page .home .icon-tips {
    width: 118rpx;
    height: 60rpx;
    padding-top: 6rpx;
    background-image: url("data:image/png;base64,");
    background-size: 100% 100%;
    text-align: center;
    position: absolute;
    left: -8rpx;
    top: -58rpx;
    z-index: 1;
    animation: jump 1s ease-in-out infinite;
}

.page .answer-head {
    height: 152rpx;
    position: relative;
}

.page .answer-head .progress {
    width: 620rpx;
    height: 36rpx;
    background-color: #d3b5ad;
    position: relative;
    border-top-left-radius: 20rpx;
    border-bottom-left-radius: 20rpx;
    overflow: hidden;
}

.page .answer-head .progress-bar {
    height: 100%;
    background-color: #f72526;
    transition: width 1s linear 0s;
}

.page .answer-head .progress-value {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
}

.page .answer-head .task {
    width: 81rpx;
    height: 98rpx;
    position: relative;
    animation: task 1s linear infinite;
    z-index: 2;
}

.page .answer-head .task .txt {
    width: 100%;
    height: 100%;
    position: absolute;
    z-index: 1;
}

.page .answer-head .light {
    position: absolute;
    right: 0rpx;
    top: 0;
    z-index: 1;
    animation: rotate 10s linear infinite;
}

.page .answer .wrapper {
    width: 650rpx;
}

.page .answer .question {
    width: 700rpx;
    height: 419rpx;
    background-image: url("data:image/png;base64,");
    background-size: 100% 100%;
}

.page .answer .question .count {
    height: 76rpx;
    line-height: 76rpx;
}

.page .answer .question .idiom {
    display: flex;
    flex-wrap: wrap;
}

.page .answer .question .idiom-item {
    width: 100rpx;
    height: 100rpx;
    margin: 0 12rpx 10rpx;
    border: 6rpx solid #a86c2f;
    background-color: #fef9e6;
    font-weight: 700;
    color: #4c4032;
    font-size: 40rpx;
    position: relative;
}

.page .answer .question .idiom .line {
    background-color: green;
    position: absolute;
}

.page .answer .question .idiom .line_horizontal {
    width: 0rpx;
    height: 6rpx;
}

.page .answer .question .idiom .line_vertical {
    width: 6rpx;
    height: 0rpx;
}

.page .answer .question .idiom .line_top {
    left: 0;
    top: -6rpx;
}

.page .answer .question .idiom .line_bottom {
    right: 0;
    bottom: -6rpx;
}

.page .answer .question .idiom .line_right {
    right: -6rpx;
    top: 0;
}

.page .answer .question .idiom .line_left {
    left: -6rpx;
    bottom: 0;
}

.page .answer .question .idiom .special .line_top {
    width: 114rpx;
    transition: width .1s ease-out 0s;
}

.page .answer .question .idiom .special .line_right {
    height: 114rpx;
    transition: height .1s ease-out .1s;
}

.page .answer .question .idiom .special .line_bottom {
    width: 114rpx;
    transition: width .1s ease-out .2s;
}

.page .answer .question .idiom .special .line_left {
    height: 114rpx;
    transition: height .1s ease-out .3s;
}

.page .answer .words {
    width: 650rpx;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-around;
}

.page .answer .words-item {
    width: 100rpx;
    height: 100rpx;
    border: 6rpx solid #a86c2f;
    background-color: #fef9e6;
    font-weight: 700;
    font-size: 40rpx;
    color: #4c4032;
    position: relative;
    margin-bottom: 10rpx;
}

.page .answer .words-item.active {
    background-color: #ffdab6;
}

.page .answer .app-banner {
    width: 710rpx;
}

.page .answer .btn-group-item {
    border: 1px dashed #d5883e;
    position: relative;
}

.page .answer .btn-group .font-14,.page .answer .btn-group .iconfont {
    color: #b3691d;
}

.page .answer .btn-group .share {
    width: 100%;
    height: 100%;
    position: absolute;
    left: 0;
    top: 0;
    z-index: 1;
    border-radius: 0;
    background-color: transparent;
}

.page .answer .banner {
    min-height: 200rpx;
}

@-webkit-keyframes task {
    0% {
        transform: rotate(0deg);
    }

    10% {
        transform: rotate(20deg) scale(1.05);
    }

    20% {
        transform: rotate(-20deg) scale(1.05);
    }

    30% {
        transform: rotate(0deg);
    }

    100% {
        transform: rotate(0deg);
    }
}

@keyframes task {
    0% {
        transform: rotate(0deg);
    }

    10% {
        transform: rotate(20deg) scale(1.05);
    }

    20% {
        transform: rotate(-20deg) scale(1.05);
    }

    30% {
        transform: rotate(0deg);
    }

    100% {
        transform: rotate(0deg);
    }
}

@-webkit-keyframes jump {
    0% {
        transform: translateY(0rpx);
    }

    15% {
        transform: translateY(-20rpx);
    }

    30% {
        transform: translateY(0rpx);
    }

    45% {
        transform: translateY(-20rpx);
    }

    60% {
        transform: translateY(0rpx);
    }

    100% {
        transform: translateY(0rpx);
    }
}

@keyframes jump {
    0% {
        transform: translateY(0rpx);
    }

    15% {
        transform: translateY(-20rpx);
    }

    30% {
        transform: translateY(0rpx);
    }

    45% {
        transform: translateY(-20rpx);
    }

    60% {
        transform: translateY(0rpx);
    }

    100% {
        transform: translateY(0rpx);
    }
}

@-webkit-keyframes flash {
    0% {
        background-color: #f72526;
    }

    100% {
        background-color: #fff;
    }
}

@keyframes flash {
    0% {
        background-color: #f72526;
    }

    100% {
        background-color: #fff;
    }
}